<Window x:Class="STCM2LEditor.FindDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:STCM2LEditor"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="FindDialog" Height="110" Width="200">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Margin="0,0,0,5" Grid.Row="0" MaxLength="20" Name="Findable"/>
        <Button Margin="5,5,5,0" Grid.Row="1" Content="Find" Click="Button_Click"/>
    </Grid>
</Window>
